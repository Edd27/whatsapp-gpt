{
  "author": "Edgar Benavides",
  "dependencies": {
    "dotenv": "16.3.1"
  },
  "description": "This GitHub project template is designed to kickstart your Node.js application development. It provides a set of essential files to help you maintain a clean and organized codebase for your Node.js project.",
  "devDependencies": {
    "@types/jest": "29.5.7",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "jest": "29.7.0",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "keywords": [
    "typescript",
    "node",
    "eslint",
    "prettier",
    "jest"
  ],
  "license": "MIT",
  "name": "node-template",
  "scripts": {
    "build": "npm-run-all format lint test clean && tsc",
    "clean": "rimraf ./build",
    "dev": "nodemon src -e ts,json --exec 'npm run local'",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "lint": "eslint src",
    "local": "ts-node src",
    "start": "node src",
    "test": "jest"
  },
  "version": "1.0.0"
}
